<fieldset
	class="ident-fieldset {%- if params.classes %} {{ params.classes }}{% endif %}"
	{%- for attribute, value in params.attributes %} {{ attribute }}="{{ value }}"{% endfor %}>
	<legend
		class="ident-fieldset__legend {%- if params.legend.classes %} {{ params.legend.classes }}{% endif %}"
		{%- for attribute, value in params.legend.attributes %} {{ attribute }}="{{ value }}"{% endfor %}>
		{{ params.legend.text }}
	</legend>
	{{ caller() if caller }}
</fieldset>
